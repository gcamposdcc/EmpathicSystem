package cl.automind.empathy.fw.arbiter;

import cl.automind.empathy.rule.AbstractArbiter;
import cl.automind.empathy.rule.AbstractRule;
import cl.automind.empathy.rule.ArbiterCriterion;

public class LessUsedCriterion extends ArbiterCriterion{

	@Override
	public AbstractRule visit(AbstractArbiter arbiter) {
		AbstractRule rule = arbiter.getRule(arbiter.getLessUsedRuleName(true));
		rule.evaluate();
		return rule;
	}

}
