package cl.automind.empathy.feedback;

public class DefaultMessage extends AbstractMessage{

	@Override
	public String getName() {
		return "DEFAULT_MESSAGE";
	}

	@Override
	public String getEmotionName() {
		return "NO_EMOTION";
	}

	@Override
	public String getUnfilteredText() {
		return "This is the default message, remember to override this message by assigning value to the preferredMessageClass in your rule";
	}

}
