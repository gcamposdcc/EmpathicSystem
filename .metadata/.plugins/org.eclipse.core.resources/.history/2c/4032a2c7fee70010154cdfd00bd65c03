package cl.automind.empathy.automind;

public class UserFeedback {
	public enum Result {
		Answered(1),
		NotShown(0),
		Timeout(-1),
		Skipped(-2);
		private int mark;
		Result(int mark){
			setMark(mark);
		}
		public void setMark(int mark){
			if(this != Answered) return;
			this.mark = mark;
		}
		public int getMark(){
			return mark;
		}
	}
	private Result result;
	private String message;
}
