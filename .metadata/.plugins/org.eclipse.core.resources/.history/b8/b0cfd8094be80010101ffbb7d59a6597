package cl.automind.empathy.data;

import cl.automind.empathy.lang.INamed;

public abstract class AIndicator<T> implements INamed{
	private String name;
	
	public AIndicator(String indicatorName){
		setName(indicatorName);
	}
	abstract T evaluate();
	protected void setName(String name) {
		this.name = name;
	}
	@Override public String getName() {
		return name;
	}
}
