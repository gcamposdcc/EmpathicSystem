package cl.automind.empathy.data;

public enum DefaultQueryOptions{
	/**
	 * Limits the number of elements
	 */
	Id(1),
	None(1),
	Filter(1),
	All(1);
	private int value;
	private DefaultQueryOptions(int value){
		this.value = Math.max(1, value);
	}
	@Override
	public String getName() {
		return this.toString();
	}

}
