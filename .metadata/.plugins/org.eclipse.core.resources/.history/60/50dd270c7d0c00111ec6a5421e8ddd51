package cl.automind.empathy.data.sql;

public class KeyValuePair<K,V> {

	private final K key;
	private V value;

	public KeyValuePair(K key){
		this.key = key;
		this.value = null;
	}

	public KeyValuePair(K key, V value){
		this.key = key;
		this.value = value;
	}

	public K getKey() {
		return key;
	}

	public V getValue() {
		return value;
	}

	public V setValue(V newValue) {
		V temp = this.value;
		this.value = newValue;
		return temp;
	}
}
